<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Code</th>
      <th>Last updated</th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.merchandise_amount %></td>
        <td><%= link_to list.url, list.url %></td>
		<td><%= list.updated_at.strftime("%m-%d-%y at %H:%M %Z") %></td>
      </tr>
    <% end %>
  </tbody>
</table>

